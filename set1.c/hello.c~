//program to add two intgers typed by the user at the keyboard
#include <iostream>
#include <stdio.h>
#include <iostream>

using namespace std;

int main(void)
{

  int X;

  cout << "Enter x" << endl;
  
  cin >> X;
  
  int total=X;

  cout << total;
  
  for(int f= 1; f< 40; f++){
    total = 1+ 1/total;
    cout << total << endl;
  }

  cout << "Hello Mac!"
  return 0;
  
}
